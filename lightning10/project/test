            om.trackClick( {'item':$(this).attr("om-event")} );
        });
        
            var mantelSkinOffset = $('#mantle_skin').offset();
            if ( mantelSkinOffset ) {
                $(window).scroll(function(e){
                    var windowScroll = $(window).scrollTop();
                    $('#ads_skinOnloadGif>div').css('top','0');
                    if(windowScroll > mantelSkinOffset.top){ 
                        $('#mantle_skin').attr('style','background-attachment: fixed !important');
                        $('#ads_skinOnloadGif>div').css('position','fixed');
                    }else{
                        $('#mantle_skin').attr('style','background-attachment: inherit !important');
                        $('#ads_skinOnloadGif>div').css('position','relative');
                    }
                    /* console.log('end skin scroll ' +windowScroll); */
                });
            }
        
